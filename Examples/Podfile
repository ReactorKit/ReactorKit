  pod 'ReactorKit', :path => '../'
  pod 'RxSwift', '~> 6.0'
  pod 'RxCocoa', '~> 6.0'

  target 'Counter' do
    project 'Counter/Counter.xcodeproj'
  end

  target 'GitHubSearch' do
    project 'GitHubSearch/GitHubSearch.xcodeproj'
  end
end
